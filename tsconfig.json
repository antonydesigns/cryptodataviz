{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "app/(pages)/dollar-cost-averaging/DCAModel.js",
    "app/(pages)/dollar-cost-averaging/page.jsx",
    "app/(utils)/utils.js",
    "app/layout.jsx",
    "app/(pages)/dollar-cost-averaging/store.js",
    "app/(pages)/(marketing)/(homepage)/page.jsx",
    "components/custom/Section.jsx",
    "app/(pages)/animate/scenes/store.js"
  ],
  "exclude": ["node_modules"]
}
